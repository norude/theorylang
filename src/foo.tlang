proc vec() -> () {
    last -> op -> last
}

proc push(array:(), new:i32) -> () {
    last -> op -> (array last op)>op new
}

proc map(array:(),op:()) -> () {
    let fold = x -> x in
    let push_op = array -> new -> push!(array, new>op) in
    fold array vec!() push_op
}

proc main() {
    let fold = x -> x in
    let array = push!(push!(vec!(), 3), 4) in
    map!(array, x -> x+1)>fold 1 x -> y -> x*y
}

