proc main() {

    let vec = last -> op -> last in
    let push = array -> new -> last -> op -> (array last op)>op new in 
    let fold = x -> x in
    let map = array -> op -> fold array vec (left -> right -> left>push right>op) in
    let array = (vec>push 3)>push 4 in

    (array>map x -> x+1)>fold 1 x -> y -> x*y

}

